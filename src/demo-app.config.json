{
  "config_id": "demo-app",
  "config_version": "1.0.0",
  "applets": {
    "irs_monitor": {
      "season_start_dates": [
        "2017-01-01",
        "2018-01-01",
        "2018-02-01",
        "2018-04-01"
      ],
      "map": {
        "chart_type": "map",
        "bin_by": "location.selection.id",
        "aggregation_names": [
          "structures sprayed %",
          "structures sprayed"
        ],
        "property_layers": [
          {
            "property": "risk",
            "label": "Risk"
          },
          {
            "property": "NumStrct",
            "label": "Number of structures"
          }
        ],
        "response_point_fields": [
          "recorded_on"
        ]
      },
      "table": {
        "chart_type": "table",
        "bin_by": "location.selection.name",
        "aggregation_names": [
          "structures sprayed",
          "structures sprayed %"
        ],
        "property_layers": [
          {
            "property": "NAME_3",
            "label": "Name"
          }
        ]
      },
      "charts": [
        {
          "id": "structures_sprayed",
          "style": {
            "height_constraint": "none",
            "width_constraint": "half"
          },
          "options": {
            "layout": {
              "showlegend": true,
              "title": "Structures found %",
              "yaxis": {
                "title": "% structures sprayed"
              },
              "xaxis": {
                "title": "Period commencing"
              }
            },
            "chart_type": "line",
            "time_series": true,
            "bin_by": "recorded_on",
            "geographic_level_refactor_this_key_name": "location.selection.id",
            "multi_series": [
              {
                "aggregation_name": "structures sprayed %"
              }
            ]
          }
        }
      ]
    },
    "irs_plan": {
      "table_output": [
        {
          "display_name": "Locality name",
          "source_field": "OBJECTID"
        },
        {
          "display_name": "Number of structures enumerated",
          "source_field": "NumStrct"
        },
        {
          "display_name": "Predicted risk",
          "source_field": "risk"
        }
      ]
    },
    "irs_record_point": {
      "metadata": {
        "show": true,
        "optional_fields": [
          "team_name"
        ]
      }
    },
    "irs_tasker": {},
    "meta": {},
    "seasons": {
      "title": "Admin"
    },
    "debug": {}
  },
  "map_focus": {
    "centre": {
      "lat": -20.239855510970585,
      "lng": 34.39481845029695
    },
    "zoom": 4.5
  },
  "instance": {
    "title": "DiSARM Yland Demo",
    "location_name": "Yland",
    "slug": "demo-app"
